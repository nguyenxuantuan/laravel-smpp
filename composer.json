{
  "name": "tuannx/laravel-smpp",
  "description": "SMS sending via SMPP protocol for Laravel framework.",
  "keywords": ["sms", "smpp", "laravel", "onlinecity", "gsm_endcode"],
  "authors": [
    {
      "name": "TuannX",
      "email": "xuantuanltv@gmail.com"
    }
  ],
  "require": {
    "ext-mbstring": "*",
    "php-smpp/php-smpp": "1.2",
    "illuminate/support": "^5|^6|^7|^8|^9.0|^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "LaravelSmpp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelSmpp\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelSmpp\\LaravelSmppServiceProvider"
      ]
    }
  }
}
